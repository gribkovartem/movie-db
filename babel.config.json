{
  "presets": ["@babel/preset-env", "@babel/preset-typescript", ["@babel/preset-react", { "runtime" : "automatic"}]],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "api": "./src/api",
        "types": "./src/types",
        "pages": "./src/pages",
        "components": "./src/components"
      }
    }]
  ],
  "ignore": ["node_modules"]
}